{
  "private": true,
  "name": "dr-server",
  "version": "0.2.0-dev.2",
  "description": "A collection of strange functions, for server",
  "author": "dr-js",
  "license": "MIT",
  "repository": "github:dr-js/dr-server",
  "bin": "bin/index.js",
  "scripts": {
    "// script ======================": "",
    "script-pack": "babel-node ./script quiet pack",
    "script-pack-test": "babel-node ./script verbose pack test",
    "script-publish": "babel-node ./script pack publish",
    "script-publish-dev": "babel-node ./script pack publish-dev",
    "script-generate-spec": "cross-env BABEL_ENV=use-source babel-node ./script/generateSpec",
    "// build =======================": "",
    "build-bin": "babel ./source-bin --out-dir ./output-gitignore/bin",
    "build-library": "babel ./source --out-dir ./output-gitignore/library",
    "build-library-dev": "cross-env BABEL_ENV=dev npm run build-library -- --watch",
    "build-module": "cross-env BABEL_ENV=module babel ./source --out-dir ./output-gitignore/module",
    "build-sample": "babel ./source-sample --out-dir ./output-gitignore/sample",
    "build-sample-dev": "cross-env BABEL_ENV=dev npm run build-sample -- --watch",
    "// test ========================": "",
    "test-server": "babel-node ./script/testServer",
    "// =============================": "",
    "lint": "eslint ./source/ ./source-bin/ ./source-sample/ ./script/",
    "test": "npm run script-pack-test",
    "prepack": "echo \"Error: pack with script-*\" && exit 1"
  },
  "engines": {
    "node": ">=10.15",
    "npm": ">=6"
  },
  "dependencies": {
    "dr-js": "^0.22.2 || ^0.22.2-dev.0"
  },
  "devDependencies": {
    "dr-dev": "0.0.6-dev.8",
    "dr-dev-babel": "0.0.6-dev.8"
  }
}
